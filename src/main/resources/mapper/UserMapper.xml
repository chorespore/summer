<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chao.summer.mapper.UserMapper">
    <select id="selectPage" resultType="com.chao.summer.domain.entity.User">
        SELECT *
        FROM user
        WHERE age > #{age}
    </select>
</mapper>
